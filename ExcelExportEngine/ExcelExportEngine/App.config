<?xml version="1.0" encoding="utf-8" ?>
<configuration>


  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <log4net debug="true">
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <file type="log4net.Util.PatternString" value="Log\ExcelExportEngine_%date{yyyyMMdd}.log"/>
      <appendToFile value="true"/>
      <bufferSize value="20"/>
      <LockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.PatternLayout">
        <header type="log4net.Util.PatternString" value="[Log Starts]%newline"/>
        <footer type="log4net.Util.PatternString" value="[Log Ends]%newline%newline"/>
        <conversionPattern value="%date [%username] - %message%newline"/>
      </layout>
    </appender>

    <!-- Specify the level for some specific categories -->
    <logger name="MyApplicationDebugLog">
      <level value="DEBUG"/>
      <appender-ref ref="LogFileAppender"/>
    </logger>
  </log4net>
  
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>

  <appSettings>
    
    <add key="strSQLConnectionString" value="server=(local); database = VEEVACRM; Integrated Security = true"/>
    <add key="strBlobConnectionString" value="DefaultEndpointsProtocol=https;AccountName=odsblobcontainer;AccountKey=EJSJZS/kQFUamEp0w70nJ6yP4CQOwiLjC8abIUtRwdD/EBsxeM3u3nmdTgqA6xrelOX1JLh3Q71WUN7wifzfYA==;EndpointSuffix=core.windows.net"/>

  </appSettings>
</configuration>