{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/api/*", "/*.{css,js,json,ico,png,jpg,svg,woff,woff2}"]
  },
  "routes": [
    {
      "route": "/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "platform": {
    "apiRuntime": "node:18"
  },
  "globalHeaders": {
    "Cache-Control": "no-cache"
  }
}
