<?xml version="1.0" encoding="utf-8" ?>
<configuration>

  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net"/>
  </configSections>

  <log4net debug="true">
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender">
      <file type="log4net.Util.PatternString" value="Log\JobMonitor_%date{yyyyMMdd}.log"/>
      <appendToFile value="true"/>
      <bufferSize value="20"/>
      <LockingModel type="log4net.Appender.FileAppender+MinimalLock"/>
      <layout type="log4net.Layout.PatternLayout">
        <header type="log4net.Util.PatternString" value="[Log Starts]%newline"/>
        <footer type="log4net.Util.PatternString" value="[Log Ends]%newline%newline"/>
        <conversionPattern value="%date [%username] - %message%newline"/>
      </layout>
    </appender>

    <!-- Specify the level for some specific categories -->
    <logger name="MyApplicationDebugLog">
      <level value="DEBUG"/>
      <appender-ref ref="LogFileAppender"/>
    </logger>
  </log4net>

  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
  </startup>

  <appSettings>


    <add key="strAzureTenantId" value="907cef94-e870-49f6-b843-09417459b152" />
    <add key="strAzureApplicationId" value="8349448f-2016-4cf0-b7c7-27c62e9be090" />
    <add key="strAzureSubscriptionId" value="3bf6616d-4f38-4fd4-82ab-51e652c757a9" />
    <add key="strAzureAuthenticationKey" value="-g09Y476-4E.itNJsSUEY5Cev5qae_3p6-" />
    <add key="strResourceGroupList" value="ODSProd,THVProdResourceGrp" />
    <add key="strAzureDataFactoryList" value="ALL" />
    
  </appSettings>
</configuration>